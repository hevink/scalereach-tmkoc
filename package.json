{
  "name": "scalereach-tmkoc",
  "scripts": {
    "dev": "nodemon",
    "dev:bun": "bun --watch run src/index.ts",
    "start": "bun run src/index.ts",
    "build": "tsc --noEmit",
    "worker": "bun run --env-file=.env src/worker.ts",
    "worker:dev": "nodemon --exec 'bun run --env-file=.env src/worker.ts' --watch src",
    "worker:watch": "bun --watch run --env-file=.env src/worker.ts",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "fix:billing-cycle": "bun run scripts/fix-billing-cycle.ts",
    "reset:workspace": "bun run scripts/reset-workspace.ts",
    "clean:jobs": "bun run src/scripts/clean-stuck-jobs.ts",
    "inspect:job": "bun run src/scripts/inspect-job.ts",
    "clear:queues": "bun run scripts/clear-queues.ts"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^3.0.44",
    "@ai-sdk/azure": "^3.0.12",
    "@ai-sdk/groq": "^3.0.11",
    "@ai-sdk/openai": "^3.0.12",
    "@asteasolutions/zod-to-openapi": "^8.4.0",
    "@aws-sdk/client-s3": "^3.970.0",
    "@aws-sdk/lib-storage": "^3.970.0",
    "@aws-sdk/s3-request-presigner": "^3.970.0",
    "@better-auth/passkey": "^1.4.15",
    "@deepgram/sdk": "^4.11.3",
    "@hono/zod-openapi": "^1.2.0",
    "@neondatabase/serverless": "^1.0.2",
    "@polar-sh/better-auth": "^1.6.4",
    "@polar-sh/sdk": "^0.42.2",
    "@sentry/node": "^10.38.0",
    "@types/archiver": "^7.0.0",
    "ai": "^6.0.39",
    "archiver": "^7.0.1",
    "axios": "^1.13.2",
    "bcryptjs": "^3.0.3",
    "better-auth": "^1.4.11",
    "bullmq": "^5.66.5",
    "dodopayments": "^2.17.2",
    "drizzle-orm": "^0.45.1",
    "groq-sdk": "^0.37.0",
    "hono": "^4.11.3",
    "ioredis": "^5.9.2",
    "nanoid": "^5.1.6",
    "nodemailer": "^7.0.12",
    "postgres": "^3.4.8",
    "sharp": "^0.34.5",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@better-auth/cli": "^1.4.10",
    "@types/bcryptjs": "^3.0.0",
    "@types/bun": "latest",
    "@types/node": "^25.0.9",
    "@types/nodemailer": "^7.0.5",
    "dotenv": "^17.2.3",
    "drizzle-kit": "0.30.0",
    "nodemon": "^3.1.11",
    "openapi-types": "^12.1.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
